#include<iostream>
using namespace std;
double fp(double k){//x平方的导数
    return 2*k;
}
double f(double data){//x平方函数
    return data*data;
}
int main(){
double xd_1=100;//x初始值
double alpha=0.9;//学习率

double xd_2=xd_1-alpha*fp(xd_1);

double y1=f(xd_1);
double y2=f(xd_2);
while((y1-y2)>0.00000001||y2-y1>0.00000001){//迭代终止条件
    xd_1=xd_2;
    xd_2=xd_1-alpha*fp(xd_1);
    y1=f(xd_1);
    y2=f(xd_2);
    cout<<y1<<" "<<xd_1<<endl;
}


}
